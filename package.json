    "@mendable/firecrawl-js": "^4.8.3",
    "ai": "^5.0.108",
    "dotenv": "^17.2.2",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "twilio": "^5.10.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "turbo": "^2.5.4"
